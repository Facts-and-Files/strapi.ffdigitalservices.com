{
  "kind": "collectionType",
  "collectionName": "timesheets",
  "info": {
    "singularName": "timesheet",
    "pluralName": "timesheets",
    "displayName": "Timesheet"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "content-manager": {
      "visible": true
    }
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "project": {
      "type": "string"
    },
    "date": {
      "type": "date",
      "required": true
    },
    "startTime": {
      "type": "time",
      "required": true
    },
    "endTime": {
      "type": "time",
      "required": true
    },
    "comment": {
      "type": "text"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "admin::user"
    },
    "totalTime": {
      "type": "decimal"
    }
  }
}
